<!DOCTYPE html>

<html lang="zxx"><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
  <%- include css3.ejs %>

    <style>
        .ht{
            height: 45px;
            width: 300px;
        }
      nav ul li{
          float:none
      }
      
    </style>
</head>
<body>

<div class="body">

    <%- include donater1nav.ejs %>

<div class="page_header">
	<div class="container">
		<div class="row">
		<div class="col-sm-6">
			<p>Change a Child's World</p>
			<h4>Available Food Donation</h4>
		</div>
		
		<div class="col-sm-6">
			<ul class="bcrumbs">
				<li><a href="/">Home</a></li>
				<li><span>Available Food Donations</span></li>
			</ul>
		</div>
		</div>
	</div>
</div>

<div class="container" style="padding: 35px;">
     
    <div class="row">
        <% for (var i =0; i <=data.length-1;  i++ ) { %>
            <div class="col-md-6 col-lg-3 col-sm-12" >

                      <div class="card">
                        <div class="card-image">
                          <img style="height: 180px;" src="download.jfif">
                          <span class="card-title">Location:<%= data[i].city %> </span>
                        </div>
                        <div class="card-content">
                          <p>
                              Food Name: <%= data[i].name %><br>
                              Food Quantity: <%= data[i].qty %><br>
                              Expiry Date: <%= data[i].date %><br>
                              PickUp Time: <%= data[i].time1 %> to <%= data[i].time2 %><br>
                          </p>
                        </div>
                        <div class="card-action">
                          <a href="accept/<%=data[i]._id  %>" name="<%=data[i]._id  %>">Accept</a>
                          &nbsp;&nbsp;
                          <a href="map/<%=data[i]._id  %>"><img style="height: 40px;" src="download2.png"></a>
                        </div>
                      </div>
                
           
              </div>
    <% } %>

</div>

  

</div>
</div>
         
<%- include footer.ejs %>
<script src="https://unpkg.com/notie"></script>



<%- include js2.ejs %>
   </body></html>